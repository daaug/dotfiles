!!! URXVT !!!
! Always call:
! xrdb -merge .Xresources

! Restore Ctrl+Shift+(c|v)
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
!URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
!URxvt.keysym.C-S-0x56: eval:paste_clipboard
URxvt.perl-ext-common: default,-confirm-paste

! Disable ISO 14755 unicode input so we can use Ctrl-Shift bindings
URxvt.iso14755: false
URxvt.iso14755_52: false

! FONT
URxvt.font: xft:Monospace:pixelsize=14
URxvt.boldfont: xft:Monospace-Bold:pixelsize=14
!URxvt.font: xft:Courier Prime:pixelsize=14
!URxvt.boldfont: xft:Courier Prime Bold:pixelsize=14

! THEME
! Remember
! Normal      Bright
!--------    -------
!0 Black      8 Black
!1 Red        9 Red
!2 Yellow    10 Yellow
!3 Green     11 Green
!4 Blue      12 Blue
!5 Magenta   13 Magenta
!6 Cyan      14 Cyan
!7 White     15 White

! special
*.foreground:   #ababab
*.background:   #000000
*.cursorColor:  #ababab

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #ff0000
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

!!! URXVT END !!!
